div(id="wrapper")
    div(id="header")
       h1
         img(src="/images/logo.png",width="235", height="78")
       div(id="nav")
            a(href="/") Home
            a(href="/event/add") add event
            a(href="/event/list") view event
     div(class="clear")
     div(id="headline")
       h2 Latest Events
     div(id="body")
      div(id="body-left")
        ul
           - each post,_id in posts
              li 
                a(href="/"+post.county+"/event/show/"+post._id)!= post.title 
                a(href="/event/delete/"+post._id)   ~ delete
      div(id="body-right")
